{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "right",
      "segments": [ 
        {
          "background": "#29315A",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b6",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": "\ue5ff {{ .Path }}",
          "trailing_diamond": "\ue0b4",
          "type": "path"
        },
        {
          "background": "#29315A",
          "foreground": "#ffffff",
          "leading_diamond": " \ue0b6",
          "properties": {
            "branch_icon": "",
            "fetch_status": true
          },
          "style": "diamond",
          "type": "git",
          "trailing_diamond": "\ue0b4"
        },
        {
          "background": "#29315A",
          "foreground": "#ffffff",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false,
            "display_default": false
          },
          "style": "diamond",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }}{{ end }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "python"
        },
        {
          "background": "#29315A",
          "foreground": "#7FD5EA",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false
          },
          "style": "diamond",
          "template": "\ufcd1{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "go"
        },
        {
          "background": "#29315A",
          "foreground": "#ffffff",
          "leading_diamond": " \ue0b6",
          "properties": {
            "precision": 2
          },
          "style": "diamond",
          "template": "\uf85a {{ round .PhysicalPercentUsed .Precision }}%",
          "trailing_diamond": "\ue0b4",
          "type": "sysinfo"
        },
        {
          "background": "#29315A",
          "foreground": "#ffffff",
          "leading_diamond": " \ue0b6",
          "properties": {
            "threshold": 150,
            "style": "austin",
            "always_enabled": true
          },
          "style": "diamond",
          "template": "{{ .FormattedMs }}",
          "trailing_diamond": "\ue0b4",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#29315A",
          "foreground": "#FFFFFF",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{ if .WSL }} {{ end }}{{.Icon}} {{ .HostName }}",
          "trailing_diamond": "\ue0b4 ",
          "type": "os"
        },
        {
          "background": "#29315A",
          "foreground": "#00DDFF",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "\uf432",
          "trailing_diamond": "\ue0b4",
          "type": "text"
        } 
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
